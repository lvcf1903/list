
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
		<!--<link href="../../../css/mui.min.css" rel="stylesheet" />-->
		<title>注册登录</title>
	</head>
	<script src="../../BaseConfig/source/js/jquery-3.0.0.min.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript">
	    var isGrChecked = false;
	    var isFrChecked = false;
	    function changeColor1(){
			document.getElementById("bt1").style.background = "#199efd";
			document.getElementById("bt2").style.background = "#ffffff";
			document.getElementById("bt1").style.color = "white";
			document.getElementById("bt2").style.color = "#199efd";
			document.getElementById("LoginGR").style.display = "block";
			document.getElementById("LoginFR").style.display = "none";
		}
	    
		function changeColor2(){
			document.getElementById("bt2").style.background = "#199efd";
			document.getElementById("bt2").style.color = "white";
			document.getElementById("bt1").style.background = "#ffffff";
			document.getElementById("bt1").style.color = "#199efd";
			document.getElementById("LoginGR").style.display = "none";
			document.getElementById("LoginFR").style.display = "block";
		}
		
		
		$(function(){
			$("#grname").width($(".content").width()-32);
			$("#grpwd").width($(".content").width()-62);
			$("#frname").width($(".content").width()-32);
			$("#frpwd").width($(".content").width()-62);
			
			$('#freye').on('click',function(){
			
		    var obj = document.getElementById("frpwd");
			if(isFrChecked){
				obj.type="password";
				isFrChecked = false;
				$('#freye').css({
				    "background-image":"url('../source/img/user_pwd_miss.png')"
			    });
			} else {
				$('#freye').css({
				    "background-image":"url('../source/img/user_pwd_show.png')"
			    });
				obj.type="text";
				isFrChecked = true;
			}	
		});
		
		$('#greye').on('click',function(){
		    var obj = document.getElementById("grpwd");
			if(isGrChecked){
				obj.type="password";
				isGrChecked = false;
				$('#greye').css({
				    "background-image":"url('../source/img/user_pwd_miss.png')"
			    });
			} else {
				$('#greye').css({
				    "background-image":"url('../source/img/user_pwd_show.png')"
			    });
				obj.type="text";
				isGrChecked = true;
			}
		});
		});  
	</script>
	<style type="text/css">
